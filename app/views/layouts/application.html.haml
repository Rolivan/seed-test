!!!
%html
  %head
    %title Seed Blocks Test
    = csrf_meta_tag
    = stylesheet_link_tag(:application, media: "all")
    = javascript_include_tag(:application)
  %body
    :javascript
      window.state = '#{@state}';
    = render partial: "partials/header"
    .wrapper
      .fit-box
        .page-box
          = render partial: "partials/main"
          = yield
    = render partial: "partials/footer"
